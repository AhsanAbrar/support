{
    "name": "ahsanabrar/support",
    "description": "My Development Supportive Package.",
    "keywords": [
        "laravel",
        "admin"
    ],
    "authors": [
        {
            "name": "Muhammad Ahsan Abrar",
            "email": "iahsanabrar@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/support": "^9.21|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.10",
        "pestphp/pest": "^2.16",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpunit/phpunit": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "AhsanDev\\Support\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AhsanDev\\Support\\Authorization\\AuthorizationServiceProvider",
                "AhsanDev\\Support\\SupportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
